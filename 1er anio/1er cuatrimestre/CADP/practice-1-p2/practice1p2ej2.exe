program practice1p2ej2;

var i,higher,num,pos,posHigher : integer;

BEGIN
	pos:= 0;
	higher:= -1;
	posHigher:=0;
	for i:= 1 to 10 do
	begin
		writeln('Enter a number');
		readln(num);
		pos:= pos + 1;
		if(num > higher) then 
		begin
			higher:= num;
			posHigher:= pos;
		end;
	end;
	writeln('The highest number is: ',higher,' and the position is: ',posHigher);
END.

